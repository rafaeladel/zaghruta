<!DOCTYPE html>
<html lang="en" xmlns:fb="http://www.facebook.com/2008/fbml">
<head>
    <!-- Meta information -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Title-->
    <title>{% block title %}Zaghruta your weeding planner guide{% endblock %}</title>

    {% block stylesheets %}
        {% stylesheets
            "bundles/zghfe/css/bootstrap.css"
            "bundles/zghfe/css/custom.css"
            "bundles/zghfe/css/jquery.fancybox.css"
            "bundles/zghfe/css/jquery.fancybox-buttons.css"
            "bundles/zghfe/css/jquery.fancybox-thumbs.css"
            filter="cssrewrite"
        %}
            <link href="{{ asset_url }}" rel="stylesheet" type="text/css" media="screen">
        {% endstylesheets %}
    {% endblock stylesheets %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
</head>
<body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        <script src="{{ asset("bundles/zghfe/js/jquery-1.11.0.js") }}"></script>
        {% javascripts
            "bundles/zghfe/js/bootstrap.min.js"
            "bundles/zghfe/js/respond.js"
            "bundles/zghfe/js/custom.js"
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>
</html>
