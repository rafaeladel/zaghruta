<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">


    <route id="zgh_fe.like.list" pattern="/like/{id}/{entity_type}" methods="get">
        <default key="_controller">ZghFEBundle:Like:getLikes</default>
    </route>

    <route id="zgh_fe.like.post" pattern="/like/{id}/{entity_type}" methods="post">
        <default key="_controller">ZghFEBundle:Like:postLike</default>
    </route>

</routes>
