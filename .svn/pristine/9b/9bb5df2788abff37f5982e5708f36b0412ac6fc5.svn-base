<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="zgh_fe.interest.get" pattern="/interests">
        <default key="_controller">ZghFEBundle:Interest:getIndex</default>
    </route>

    <route id="zgh_fe.interest.post" pattern="/interests/post">
        <default key="_controller">ZghFEBundle:Interest:postInterest</default>
    </route>

</routes>
