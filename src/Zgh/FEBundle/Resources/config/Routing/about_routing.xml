<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="zgh_fe.about.get_customer_edit" pattern="/about/{id}/customer/edit" methods="GET">
        <default key="_controller">ZghFEBundle:About:getCustomerEdit</default>
    </route>

    <route id="zgh_fe.about.post_customer_edit" pattern="/about/{id}/customer/edit" methods="POST">
        <default key="_controller">ZghFEBundle:About:postCustomerEdit</default>
    </route>

    <route id="zgh_fe.about.get_vendor_edit" pattern="/about/{id}/vendor/edit" methods="GET">
        <default key="_controller">ZghFEBundle:About:getVendorEdit</default>
    </route>

    <route id="zgh_fe.about.post_vendor_edit" pattern="/about/{id}/vendor/edit" methods="POST">
        <default key="_controller">ZghFEBundle:About:postVendorEdit</default>
    </route>


    <route id="zgh_fe.about.accept_relationship" pattern="/relationship/{id}/accept/{n_id}">
        <default key="_controller">ZghFEBundle:About:acceptRelationship</default>
    </route>

    <route id="zgh_fe.about.deny_relationship" pattern="/relationship/{id}/deny/{n_id}">
        <default key="_controller">ZghFEBundle:About:denyRelationship</default>
    </route>

</routes>
