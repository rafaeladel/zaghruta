{% for product in wishlist.products %}
    <div class="product-G col-md-3 col-xs-6  col-sm-6">
        <div class="thumbnail clearfix">
            <a data-fancybox-href="{{ url("zgh_fe.products.get_parent_content_widget", {"id": product.user.id, "product_id": product.id}) }}" href="{{ url("zgh_fe.products.content", {"id": product.user.id, "product_id": product.id}) }}" class="textImageContainer fancybox  fancybox.ajax">
                <div class="thumbOverlay img-responsive">
                    <div class="thumbOverlay"><span class="glyphicon glyphicon-eye-open"></span></div>
                </div>
                <img class="img-responsive" src="{{ product.getWebPath() ? asset(product.getWebPath()) : asset("bundles/zghfe/img/thumb.jpg") }}">
            </a>
        </div>
    </div>
{% endfor %}